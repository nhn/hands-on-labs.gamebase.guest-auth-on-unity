<?xml version='1.0'?>
<Gamebase>
  <downloadPath></downloadPath>
  <useUnityAdapter>true</useUnityAdapter>
  <useAndroidPlatform>true</useAndroidPlatform>
  <useIOSPlatform>true</useIOSPlatform>  
  <base>
    <unity>
      <directories>
        <directory>gamebase-sdk</directory>
      </directories>
      <isSettings>false</isSettings>
    </unity>
    <ios>
      <directories>
        <directory>Gamebase.bundle</directory>
        <directory>Gamebase.framework</directory>
      </directories>
      <isSettings>false</isSettings>
    </ios>
    <android>
      <directories>
        <directory>gamebase-sdk</directory>
      </directories>
      <isSettings>false</isSettings>
    </android>
  </base>
  <adapters>
<!--Authentication-->
    <adapter>
      <category>Authentication</category>
      <name>Facebook</name>
      <unity>
        <directory>gamebase-adapter-auth-facebook</directory>
        <useAdapter>false</useAdapter>
        <title>Facebook SDK Download</title>
        <desc>Facebook Unity SDK is required.

Please download and install SDK after completing Gamebase settings.

If you want to use suitable version of Facebook Unity SDK for adapter, Please refer to README file supplied with the adapter.</desc>
        <button_ok>SDK Download</button_ok>
        <button_close>Close</button_close>
        <link>https://developers.facebook.com/docs/unity/downloads</link>
      </unity>
      <ios>
        <directory>GamebaseAuthFacebookAdapter.framework</directory>
        <externals>FacebookSDK</externals>
        <useAdapter>false</useAdapter>
      </ios>
      <android>
        <directory>gamebase-adapter-auth-facebook</directory>
        <useAdapter>false</useAdapter>
      </android>
      <groupBySelectOnlyOneForCategory>false</groupBySelectOnlyOneForCategory>
      <isUnityPriority>false</isUnityPriority>
      <unityOrNativeParallelUseDisable>true</unityOrNativeParallelUseDisable>
      <isSettings>false</isSettings>
    </adapter>
    <adapter>
      <category>Authentication</category>
      <name>Google</name>      
      <ios>
        <directory>GamebaseAuthGoogleAdapter.framework</directory>        
        <useAdapter>false</useAdapter>
      </ios>
      <android>
        <directory>gamebase-adapter-auth-google</directory>
        <useAdapter>false</useAdapter>
      </android>
      <groupBySelectOnlyOneForCategory>false</groupBySelectOnlyOneForCategory>
      <isUnityPriority>false</isUnityPriority>
      <unityOrNativeParallelUseDisable>true</unityOrNativeParallelUseDisable>
      <isSettings>false</isSettings>
    </adapter>
    <adapter>
      <category>Authentication</category>
      <name>Gamecenter</name>
      <ios>
        <directory>GamebaseAuthGamecenterAdapter.framework</directory>
        <useAdapter>false</useAdapter>
      </ios>
      <groupBySelectOnlyOneForCategory>false</groupBySelectOnlyOneForCategory>
      <isUnityPriority>false</isUnityPriority>
      <unityOrNativeParallelUseDisable>false</unityOrNativeParallelUseDisable>
      <isSettings>false</isSettings>
    </adapter>
    <adapter>
      <category>Authentication</category>
      <name>Payco</name>      
      <ios>
        <directory>GamebaseAuthPaycoAdapter.framework</directory>
        <externals>PIDThirdPartyAuth</externals>
